$atoms
    basis =def-TZVP
ce 2,18
    ecp   =ce def-ecp
$basis    file=basis
$ecp    file=basis
$scfmo   file=mos
$scfiterlimit       100
$scfdamp   start=  1.500  step=  0.050  min=  0.100
$scfdump
$scfdiis
$maxcor    500 MiB  per_core
$scforbitalshift   noautomatic   closedshell=0.5000
$rundimensions
   natoms=41
$last SCF energy change = -.14676799
$charge from dscf
          1.000 (not to be modified here)
$closed shells
 a       1-141                                  ( 2 )
$energy    file=energy
$grad    file=gradient
$dft
    functional   b-p
    gridsize   m4
$scfconv   7
$dipole from dscf
  x    -1.80550874288735    y     0.22297061389484    z     7.29079280721612    a.u.
   | dipole | =   19.0996799917  debye
$last step     dscf
$orbital_max_rnorm 0.19928859835252E-03
$end
