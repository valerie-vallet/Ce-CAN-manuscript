   radius=  1.3000
$cosmo_out file=CeIV-O-CeIV-eclipsed-nono3.cosmo
$cosmo_data file=cosmo_transfer.tmp
$title
$symmetry c1
$coord    file=coord
$optimize
 internal   off
 redundant  off
