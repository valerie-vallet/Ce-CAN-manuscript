$scfiterlimit       30
$scfdamp   start=0.300  step=0.050  min=0.100
$scfdump
$scfdiis
$maxcor    500 MiB  per_core
$scforbitalshift  automatic=.1
$energy    file=energy
$grad    file=gradient
$dft
    functional   b-p
    gridsize   m4
$scfconv   7
$rundimensions
   natoms=3
$last step     dscf
$last SCF energy change = 0.12875331E-01
$charge from dscf
          0.000 (not to be modified here)
$dipole from dscf
  x    -0.00000000007530    y     0.83509773888844    z     0.00000003605872    a.u.
   | dipole | =    2.1226227722  debye
$orbital_max_rnorm 0.15173135882503E-05
