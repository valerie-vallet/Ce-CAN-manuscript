$cosmo_atoms
# radii in Angstrom units
o  1                                                                           \
   radius=  1.7200
h  2                                                                           \
   radius=  1.3000
$cosmo_out file=oh-m.cosmo
$cosmo_data file=cosmo_transfer.tmp
$title
$symmetry c1
$coord    file=coord
$optimize
 internal   off
 redundant  off
 cartesian  on
$atoms
    basis =def-TZVP
$basis    file=basis
$scfmo   file=mos
$closed shells
 a       1-5                                    ( 2 )
$scfiterlimit       30
$scfdamp   start=0.300  step=0.050  min=0.100
$scfdump
$scfdiis
$maxcor    500 MiB  per_core
$scforbitalshift  automatic=.1
$energy    file=energy
$grad    file=gradient
$dft
    functional   b-p
    gridsize   m4
$scfconv   7
$rundimensions
   natoms=2
$last step     dscf
$last SCF energy change = 0.15361727
$charge from dscf
         -1.000 (not to be modified here)
$dipole from dscf
  x     0.00000000124812    y     0.23239870938149    z     0.11619994215146    a.u.
   | dipole | =    0.6604267701  debye
$orbital_max_rnorm 0.10464733536805E-04
$end
